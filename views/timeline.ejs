<!-- posts section section -->
	<section>
		<div class="gap">
			<div class="container">
				<div class="row">
					<div class="col-12">


					<% posts.forEach((item)=>{ %>
						<div class="user-post">
								<figure>
									<% if( users){ %>
										<img src="/upload2/<%= users.avatar %>" alt="">
										<% } %>
								</figure>
							<div class="user-name">
								<h5><%= users.fullname %></h5>
								<span>1 weeks ago</span>
							</div>
							<div class="post-meta">
								<p>
									<%= item.description %>
								</p>
								
									
								<!-- <img src="/src/src/uploads/file_1607426940784.jpg" alt=""> -->
								<% item.image.forEach((one)=>{ %>
									<% if (one.match(/.(jpg|jpeg|png|gif)$/i)) { %>
									<img src="/uploads/<%= one %>" alt="">
									<%} else { %>
									<video width="500" height="240" autoplay controls>
										
										<source src="/uploads/<%= one %>" type=video/mp4>
									  </video>
									  <% } %>
								<% }) %>
							</div>
							<div class="more ico-hover">
								<i class="lni lni-more"></i>
								<div class="more-options">
									<ul>
										<li><i class="lni lni-trash"></i> Delete Post</li>
										<li><i class="lni lni-ban"></i> Hide Post</li>
										<li><i class="lni lni-pencil-alt"></i> Edit Post</li>
									</ul>
								</div>
							</div>
							<div class="user-stat">
								<div class="emoji-state">
									<img src="/images/svg/thumb.png" alt="">
									<img src="/images/svg/heart.png" alt="">
									<img src="/images/svg/smile.png" alt="">
									<img src="/images/svg/weep.png" alt="">
									<p><%= item.likes.length %></p>
								</div>
								<div class="coment-state">
									<span><i class="lni lni-comments"></i><%= item.comments.length %> comments</span>
									<span><i class="lni lni-share"></i><%= item.shared.length %> shares</span>
								</div>
							</div>
							<div class="stat-tools">
								<div class="box">
									<div class="Like"><a class="Like__link"><i class="lni lni-heart"></i>
										<form action="/userprofile/<%= item._id %>/likes" method="POST">
											<button id="likebutton" type="submit" style="border: none; background-color:white;">Like</button>
										</form>
									</a>
									<div class="Emojis">
									  <div class="Emoji Emoji--like">
										<div class="icon icon--like"></div>
									  </div>
									  <div class="Emoji Emoji--love">
										<div class="icon icon--heart"></div>
									  </div>
									  <div class="Emoji Emoji--haha">
										<div class="icon icon--haha"></div>
									  </div>
									  <div class="Emoji Emoji--wow">
										<div class="icon icon--wow"></div>
									  </div>
									  <div class="Emoji Emoji--sad">
										<div class="icon icon--sad"></div>
									  </div>
									  <div class="Emoji Emoji--angry">
										<div class="icon icon--angry"></div>
									  </div>
									</div>
								  </div>
								</div>
								<a class="comment-to" href="#" title=""><i class="lni lni-comments"></i> comment</a>
								<a class="share-to" href="#" title=""><i class="lni lni-share-alt"></i> share</a>
								<div class="new-comment">
									<!-- comment section section -->
									<form action="/userprofile/<%= item._id %>/comment"method="post">
										<input type="text" name="content" placeholder="write comment">
										<button type="submit"><i class="lni lni-pointer"></i></button>
									</form>
									
									<!-- comment section section -->
								</div>
								<form action="/userprofile/<%= item._id %>/share" method="post">
							
									<button type="submit"><i class="lni lni-pointer"></i></button>
								</form>
								<div class="box">
									<div class="Emojis">
									  <div class="Emoji Emoji--like">
										<div class="icon icon--like"></div>
									  </div>
									  <div class="Emoji Emoji--love">
										<div class="icon icon--heart"></div>
									  </div>
									  <div class="Emoji Emoji--haha">
										<div class="icon icon--haha"></div>
									  </div>
									  <div class="Emoji Emoji--wow">
										<div class="icon icon--wow"></div>
									  </div>
									  <div class="Emoji Emoji--sad">
										<div class="icon icon--sad"></div>
									  </div>
									  <div class="Emoji Emoji--angry">
										<div class="icon icon--angry"></div>
									  </div>
									</div>
								  </div>
							</div>
							<!-- comment section section -->
							<div class="comments-area">
								<ul>

									<% item.comments.forEach((once)=>{ %>
										<li>
											<figure>
												<% if( users){ %>
													<img src="/upload2/<%= users.avatar %>" alt="">
													<% } %>
											</figure>
											<div class="commenter">
												<h5><a href="#" title=""><%= once.usercommented.fullname %></a></h5>
												<span>2 hours ago</span>
												<p>
													<%= once.content %>
												</p>
											</div>
											<a href="#" title="Like"><i class="lni lni-heart"></i></a>
											<a class="reply-coment" href="#" title="Reply"><i class="lni lni-reply"></i></a>
										</li>
									<% }) %>


								</ul>
							</div>
							<!-- comment section section -->
						</div><!-- with image -->
					<% }) %>

						<div class="loadmore">
							<div class="loader">
								<div class="line"></div>
								<div class="line"></div>
								<div class="line"></div>
								<div class="line"></div>
							</div>
							<span>Loading...</span>
						</div><!-- load more -->
					</div>
				</div>
			</div>
		</div>
	</section>
	
